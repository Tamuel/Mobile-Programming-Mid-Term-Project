<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.softwork.ydk.middletermproject_time_table" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".TimeTableActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MakeTimeTableActivity" >
        </activity>
        <provider
            android:name=".TimeTableDBProvider"
            android:authorities="middletermproject_time_table.com"
            android:exported="true"></provider>
    </application>

</manifest>
